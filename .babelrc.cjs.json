{
  "presets": [
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "browserslistEnv": "node"
      }
    ],
    "@babel/flow",
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@utils": "./src/utils/"
        }
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
