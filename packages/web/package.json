{
  "name": "@mui-tabs/web",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:node && yarn build:modern && yarn build:styles && yarn build:types",
    "build:node": "node ../../scripts/build.mjs --env node --out-dir ./",
    "build:modern": "node ../../scripts/build.mjs --env modern --out-dir ./esm",
    "build:types": "node ../../scripts/buildTypes.mjs --out-dir ./ && node ../../scripts/buildTypes.mjs --out-dir ./esm",
    "build:styles": "node ../../scripts/buildStyles.mjs --source-dir ./styles --out-dir ./dist --copy-source-to ./scss",
    "test": "NODE_ENV=test mocha --exit"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@mui-tabs/utils": "1.0.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/chai": "^4.3.0",
    "@types/chai-dom": "^0.0.12",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.12.38",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.4",
    "@types/sinon": "^10.0.11",
    "@types/styled-components": "^5.1.24",
    "chai": "^4.3.6",
    "chai-dom": "^1.11.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.2",
    "prismjs": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^3.4.1",
    "sass": "^1.49.9",
    "sinon": "^13.0.1",
    "styled-components": "^5.3.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
