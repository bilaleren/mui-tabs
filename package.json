{
  "name": "mui-tabs",
  "version": "0.0.3",
  "description": "Material UI tabs for your projects.",
  "author": "Bilal Eren",
  "license": "MIT",
  "repository": "https://github.com/bilaleren/mui-tabs.git",
  "main": "index.js",
  "homepage": "https://bilaleren.github.io/mui-tabs",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "tabs",
    "mui tabs",
    "react tabs",
    "material-ui",
    "material ui",
    "material tabs",
    "material ui tabs",
    "react material tabs"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "start": "rollup -c -w",
    "test": "yarn test:lint && yarn test:unit",
    "fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "test:lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test:unit": "NODE_ENV=test mocha --exit",
    "test:watch": "NODE_ENV=test mocha --watch",
    "predeploy": "cd example && rm -rf build && yarn run build:example",
    "deploy": "node scripts/deploy-gh-pages.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,scss}\" \"example/src/**/*.{ts,tsx,js,scss}\"",
    "publish:package": "yarn version --patch && yarn build && node scripts/publish-package.js",
    "unlink:package": "unlink $PWD/node_modules/mui-tabs",
    "link:package": "ln -sf $PWD/dist $PWD/node_modules/mui-tabs"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "dependencies": {
    "clsx": "^1.1.1",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/chai": "^4.3.0",
    "@types/chai-dom": "^0.0.12",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.12.38",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-transition-group": "^4.4.4",
    "@types/sinon": "^10.0.11",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^10.4.4",
    "chai": "^4.3.6",
    "chai-dom": "^1.11.0",
    "cssnano": "^5.1.7",
    "dotenv": "^16.0.0",
    "eslint": "^6.8.0",
